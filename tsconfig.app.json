{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "electron-main/**/*.ts", "electron-preload/**/*.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "types": ["element-plus/global.d.ts"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
